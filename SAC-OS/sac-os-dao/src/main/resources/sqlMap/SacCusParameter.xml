  	<select id="listSacCusParameterByCusName" parameterClass="java.util.Map" resultClass="java.lang.String">  
  	SELECT * FROM
		( SELECT e.*, ROWNUM R FROM
		(  	
      SELECT 
	       distinct(cus_name) as cusName
	        from SAC_CUS_PARAMETER  
        <dynamic prepend="WHERE">
          <isNotEmpty prepend="AND" property="cusName"> cus_name like '%$cusName$%' </isNotEmpty>             
        </dynamic>
        ) e
        <![CDATA[
        WHERE ROWNUM <= #end# ) f
		WHERE R > #start#   
		]]> 
