  <select id="getSacRecDifferencess" parameterClass="java.util.Map" resultMap="SacRecDifferences">  
      SELECT 
	       id,
	       rec_detail_id,
	       rec_start_date,
	       rec_end_date,
	       trx_serial_no,
	       rec_diff_type,
	       rec_diff_desc,
	       chn_code,
	       status,
	       deal_type,
	       deal_oper,
	       create_time,
	       memo,
	       rec_batch_id,
	       trx_time,
	       pay_amount,
	       currency_type,
	       bank_serial_no,
	       paycon_type,
	       update_time,
	       ori_trx_state,
	       ori_inn_con_amount,
	       ori_cha_con_amount,
	       trx_code,
	       priv_domain,
	       supplement_flag,
	       rec_oper
	  FROM SAC_REC_DIFFERENCES
        <dynamic prepend="WHERE">
          <isNotEmpty prepend="AND" property="oneMonthFlag"> 
			<![CDATA[ 
			CREATE_TIME >=  TRUNC(add_months(sysdate,-1),'dd') 
			]]>
		  </isNotEmpty>
		  <isNotEmpty prepend="AND" property="oneDayFlag"> 
			<![CDATA[ 
			CREATE_TIME >=  TRUNC(sysdate,'dd') 
			]]>
		  </isNotEmpty>
          <isNotEmpty prepend="AND" property="id"> ID = #id# </isNotEmpty> 
          <isNotEmpty prepend="AND" property="recDiffType"> rec_diff_type = #recDiffType# </isNotEmpty> 
          <isNotEmpty prepend="AND" property="trxSerialNo"> trx_serial_no = #trxSerialNo# </isNotEmpty>  
          <isNotEmpty prepend="AND" property="recDetailId"> rec_detail_id = #recDetailId# </isNotEmpty>
          <isNotEmpty prepend="AND" property="bankSerialNo">  bank_serial_no = #bankSerialNo#  </isNotEmpty>
          <isNotEmpty prepend="AND" property="beginDate"> 
			<![CDATA[ 
			CREATE_TIME >= to_date(#beginDate#,'yyyy-MM-dd')
			]]>
		  </isNotEmpty>
		  <isNotEmpty prepend="AND" property="endDate">
			<![CDATA[ 
			CREATE_TIME < to_date(#endDate#,'yyyy-MM-dd') 
			]]> 
		  </isNotEmpty> 
		  <isNotEmpty prepend="AND" property="status">   status = #status#  </isNotEmpty>	
		  <isNotEmpty prepend="AND" property="supplementFlag">  supplement_flag = #supplementFlag#  </isNotEmpty>
		  <isNotEmpty prepend="AND" property="notSuccStatus">  <![CDATA[  status <> #notSuccStatus# ]]> </isNotEmpty>                   
        </dynamic>
		  <isEmpty prepend="ORDER BY" property="orderByFlag"> CREATE_TIME DESC </isEmpty>
          <isEqual prepend="ORDER BY" property="orderByFlag" compareValue="0"> trx_time DESC </isEqual>
          <isEqual prepend="ORDER BY" property="orderByFlag" compareValue="1"> trx_time ASC </isEqual>
  </select>
  
  <select id="selectDifferencesForSupplement" parameterClass="java.util.Map" resultMap="SacRecDifferences">  
      SELECT 
	       a.id,
	       a.rec_detail_id,
	       a.rec_start_date,
	       a.rec_end_date,
	       a.trx_serial_no,
	       '(' || a.rec_diff_type || ')' || DECODE(b.rec_diff_type, a.rec_diff_type, NULL, NULL, NULL, '(' || b.rec_diff_type || ')') AS "rec_diff_type",
	       '(' || a.rec_diff_desc || ')' || DECODE(b.rec_diff_desc, a.rec_diff_desc, NULL, NULL, NULL, '(' || b.rec_diff_desc || ')') AS "rec_diff_desc",
	       a.chn_code,
	       a.status,
	       a.deal_type,
	       a.deal_oper,
	       a.create_time,
	       a.memo,
	       a.rec_batch_id,
	       a.trx_time,
	       a.pay_amount,
	       a.currency_type,
	       a.bank_serial_no,
	       a.paycon_type,
	       a.update_time,
	       a.ori_trx_state,
	       a.ori_inn_con_amount,
	       a.ori_cha_con_amount,
	       a.trx_code,
	       a.priv_domain,
	       a.supplement_flag,
	       a.rec_oper
	  FROM SAC_REC_DIFFERENCES a
      LEFT JOIN sac_rec_differences b ON (a.trx_serial_no = b.trx_serial_no  
		      AND a.rec_diff_type != b.rec_diff_type 
		      AND a.supplement_flag=b.supplement_flag) 
	  
        <dynamic prepend="WHERE">
          <isNotEmpty prepend="AND" property="recDiffType"> a.rec_diff_type = #recDiffType# </isNotEmpty> 
		  <isNotEmpty prepend="AND" property="status">   a.status = #status#  </isNotEmpty>	
		  <isNotEmpty prepend="AND" property="supplementFlag">  a.supplement_flag = #supplementFlag#  </isNotEmpty>
		  <isNotEmpty prepend="AND" property="bothsupplementFlag">  
		     a.rec_diff_type = 'INN001' and b.rec_diff_type = 'CHA002' 
		  </isNotEmpty>                   
        </dynamic>
  </select>
  
