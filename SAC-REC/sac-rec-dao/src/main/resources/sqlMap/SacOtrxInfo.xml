	  <select id="getAmountGroupByCus" parameterClass="java.util.Map" resultClass="java.util.HashMap">  
      SELECT 
	       SUM(PAY_AMOUNT) AS "payAmounts",
	       SUM(TAX_AMOUNT) AS "taxAmounts",
	       $groupFields$ AS "cardId"
	  FROM SAC_OTRX_INFO
        <dynamic prepend="WHERE">
          <isNotEmpty prepend="AND" property="trxState">  trx_state = #trxState#  </isNotEmpty> 
          <isNotEmpty prepend="AND" property="trxType">  trx_type = #trxType#  </isNotEmpty>
          <isNotEmpty prepend="AND" property="trxSuccTime"> 
			<![CDATA[ 
			(
			trx_succ_time >= to_date(#trxSuccTime#,'yyyyMMdd')
			AND
			trx_succ_time< to_date(#trxSuccTime#,'yyyyMMdd')+1
			)
			]]>
		  </isNotEmpty>
        </dynamic>
		 GROUP　BY $groupFields$
  </select>
   
	<select id="getRefundAmountGroupByCus" parameterClass="java.util.Map" resultClass="java.util.HashMap">  
      SELECT 
	       SUM(a.PAY_AMOUNT) AS "payAmounts",
	       sum(decode(a.PAY_AMOUNT,c.PAY_AMOUNT,0,a.TAX_AMOUNT)) AS "taxAmounts",
	       b.$groupFields$ AS "cardId"
	  FROM SAC_OTRX_INFO a,SAC_OTRX_INFO b,SAC_OTRX_INFO C
        WHERE
        	a.trx_serial_no = b.otrx_serial_no
        	and a.otrx_serial_no = c.trx_serial_no
        	and a.trx_type='3303' 
        	and b.trx_type='3423'
        	and c.trx_type='3302'
          <isNotEmpty prepend="AND" property="trxState">  b.trx_state = #trxState#  </isNotEmpty> 
          <isNotEmpty prepend="AND" property="trxSuccTime"> 
			<![CDATA[ 
			(
			b.trx_succ_time >= to_date(#trxSuccTime#,'yyyyMMdd')
			AND
			b.trx_succ_time< to_date(#trxSuccTime#,'yyyyMMdd')+1
			)
			]]>
		  </isNotEmpty>
		 GROUP　BY b.$groupFields$
  </select>
  
